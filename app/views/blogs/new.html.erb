<div class="container">
  <div class="row">
    <div class="col-xs-3">
    <%= render 'users/profile', user: current_user %>
    </div>
    <h1>新規投稿</h1>
    <%= form_with model: @blog, local:true do |f| %>
      <h4>タイトル</h4>
      <%= f.text_field :title, class: "form-control" %>

      <h4>画像</h4>
      <%= f.attachment_field :image %>
      <h4>本文</h4>
      <%= f.text_area :body %>
      <%= f.submit '投稿' %>
    <% end %>
 </div>
</div>